---
// Case Study section component extracted from index.astro
---

<section class="case-study-section section-spacing">
  <div class="w-layout-blockcontainer container w-container">
    <div
      data-w-id="5ac1d744-e37e-7e27-1f78-5cf796387d59"
      class="section-title section-title-wrap">
      <h2>Patient success stories</h2>
      <a href="case-study.html" class="button-primary-1-outline w-button"
        >View all case study</a
      >
    </div>
  </div>
  <div
    data-w-id="9f2376ba-ef6b-a8aa-4f1e-62e632027489"
    class="scroll-case-study-wrapper">
    <div class="scroll-case-study-wrap">
      <div class="case-study-collection-list-wrapper w-dyn-list">
        <div role="list" class="case-study-wrap w-dyn-items">
          <div role="listitem" class="case-study-collection-item w-dyn-item">
            <a
              data-w-id="bd614e94-90ba-b978-a1ae-1b1eb1be6300"
              href="#"
              class="grid-case-study-item w-inline-block">
              <div class="scroll-case-study-image-wrap">
                <img
                  src="https://d3e54v103j8qbb.cloudfront.net/plugins/Basic/assets/placeholder.60f9b1840c.svg"
                  loading="eager"
                  style="-webkit-transform:translate3d(0, 0, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);-moz-transform:translate3d(0, 0, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);-ms-transform:translate3d(0, 0, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);transform:translate3d(0, 0, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0)"
                  alt=""
                  class="scroll-case-study-image w-dyn-bind-empty"
                />
              </div>
              <div class="scroll-case-study-content-wrap">
                <div>
                  <h3
                    style="color:rgb(9,19,49)"
                    class="scroll-case-study-title w-dyn-bind-empty">
                  </h3>
                  <p class="w-dyn-bind-empty"></p>
                </div>
                <div
                  data-w-id="831d64a9-a22a-82f5-01ed-677d23838717"
                  class="button-arrow">
                  <div>View case study</div>
                  <div class="button-arrow-wrap">
                    <img
                      src="arrow-01.svg"
                      loading="lazy"
                      style="-webkit-transform:translate3d(0px, 0, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);-moz-transform:translate3d(0px, 0, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);-ms-transform:translate3d(0px, 0, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);transform:translate3d(0px, 0, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0)"
                      alt=""
                      class="button-arrow-icon"
                    /><img
                      src="arrow-01.svg"
                      loading="lazy"
                      style="-webkit-transform:translate3d(-70px, 0, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);-moz-transform:translate3d(-70px, 0, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);-ms-transform:translate3d(-70px, 0, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);transform:translate3d(-70px, 0, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0)"
                      alt=""
                      class="button-arrow-hover-icon"
                    />
                  </div>
                </div>
              </div>
            </a>
          </div>
        </div>
        <div class="empty-state w-dyn-empty">
          <div>No items found.</div>
        </div>
      </div>
    </div>
  </div>
</section>
