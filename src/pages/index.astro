---
import Navbar from "../components/Navbar.astro";
import HeroSection from "../components/HeroSection.astro";
import Botox from "../components/Botox.astro";
import Ablauf from "../components/Ablauf.astro";
import PostTreatment from "../components/PostTreatment.astro";
import Prices from "../components/Prices.astro";
import FaqSection from "../components/FaqSection.astro";
import Contact from "../components/Contact.astro";
import AppointmentSection from "../components/AppointmentSection.astro";
import Footer from "../components/Footer.astro";
---

<!doctype html>
<html lang="de">
  <head>
    <meta charset="utf-8" />
    <link rel="icon" type="image/svg+xml" href="/favicon.ico" />
    <meta name="viewport" content="width=device-width" />
    <meta name="generator" content={Astro.generator} />
    <title>Dr. Scheffbuch Ästhetische Medizin - St. Gallen</title>
    <meta
      content="Dr. Scheffbuch Ästhetische Medizin in St. Gallen. Professionelle Botulinumtoxin-Behandlungen und ästhetische Medizin für natürliche Ergebnisse."
      name="description"
    />
    <meta content="Dr. Scheffbuch Ästhetische Medizin" property="og:title" />
    <meta
      content="Dr. Scheffbuch Ästhetische Medizin in St. Gallen. Professionelle Botulinumtoxin-Behandlungen und ästhetische Medizin für natürliche Ergebnisse."
      property="og:description"
    />
    <meta content="https://drscheffbuch.ch/og-image.jpg" property="og:image" />
    <meta
      content="Dr. Scheffbuch Ästhetische Medizin"
      property="twitter:title"
    />
    <meta
      content="Dr. Scheffbuch Ästhetische Medizin in St. Gallen. Professionelle Botulinumtoxin-Behandlungen und ästhetische Medizin für natürliche Ergebnisse."
      property="twitter:description"
    />
    <meta
      content="https://drscheffbuch.ch/og-image.jpg"
      property="twitter:image"
    />
    <meta property="og:type" content="website" />
    <meta content="summary_large_image" name="twitter:card" />
    <link href="/normalize.css" rel="stylesheet" type="text/css" />
    <link href="/webflow.css" rel="stylesheet" type="text/css" />
    <link href="/main.css" rel="stylesheet" type="text/css" />
    <link href="https://fonts.googleapis.com" rel="preconnect" />
    <link
      href="https://fonts.gstatic.com"
      rel="preconnect"
      crossorigin="anonymous"
    />
    <script
      src="https://ajax.googleapis.com/ajax/libs/webfont/1.6.26/webfont.js"
      type="text/javascript"></script>
    <script type="text/javascript">
      WebFont.load({ google: { families: ["Outfit:regular,500,600"] } });
    </script>
    <script type="text/javascript">
      !(function (o, c) {
        var n = c.documentElement,
          t = " w-mod-";
        (n.className += t + "js"),
          ("ontouchstart" in o ||
            (o.DocumentTouch && c instanceof DocumentTouch)) &&
            (n.className += t + "touch");
      })(window, document);
    </script>
    <link href="/icon.JPG" rel="shortcut icon" type="image/x-icon" />
    <link href="/webclip.png" rel="apple-touch-icon" />
  </head>
  <body>
    <Navbar />
    <HeroSection />
    <Botox />
    <Ablauf />
    <PostTreatment />
    <Prices />
    <FaqSection />
    <Contact />
    <AppointmentSection />
    <Footer />
    <script
      src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=68711d07d3dab3bc36b1b545"
      type="text/javascript"
      integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0="
      crossorigin="anonymous"></script>
    <script src="/webflow.js" type="text/javascript"></script>
  </body>
</html>
